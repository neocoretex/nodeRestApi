{
  "serializer":"./serializers/html-serializer",
  "rules":{
    "/feedback/:id":{
      "db":"feedback",
      "modelName":"feedback",
      "mixin":"./mixin/listViewDeleteMixin",
      "fields":["email","content"]
    },
     "/feedback":{
      "db":"feedback",
      "modelName":"feedback",
      "mixin":"./mixin/listViewMixin",
      "fields":["email","content"],
      "filter":{
        "email":"test@test.com"
      }
    },
    "/blog":{
      "db":"blog",
      "modelName":"blog",
      "mixin":"./mixin/listViewMixin",
      "fields":["title","markdown"],
      "filter":{}
    }
  }
}
